<html>
<head>
    <meta charset="utf-8" />
    <title>oauth2 client list</title>
</head>
<body>
<h1>oauth2 client list</h1>

<a href="/">Top</a>
<a href="/admin/oauth2/clients/new">New</a>

<table border=1>
    <thead>
        <tr>
            <th>id</th>
            <th>name</th>
            <th>description</th>
            <th>callback url</th>
            <th>created at</th>
        </tr>
    </thead>
    <tbody>
        {{ range .Clients }}
        <tr>
            <td><a href="/admin/oauth2/clients/{{ .ID }}">{{ .ID }}</a></td>
            <td>{{ .Name }}</td>
            <td>{{ .Description }}</td>
            <td>{{ .CallbackURL }}</td>
            <td>{{ .CreatedAt }}</td>
        </tr>
        {{ end }}
    </tbody>
</table>
</body>
</html>